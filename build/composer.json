{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of womenactivists.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/womenactivists.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://malsup.github.io/jquery.cycle.all.js"
        },
        "name": "malsup/jquery.cycle",
        "require": {
          "composer/installers": "^1.6.0"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://raw.githubusercontent.com/briancherne/jquery-hoverIntent/master/jquery.hoverIntent.js"
        },
        "name": "briancherne/jquery.hoverintent",
        "require": {
          "composer/installers": "^1.6.0"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://cdnjs.cloudflare.com/ajax/libs/json2/20160511/json2.js"
        },
        "name": "cdnjs/json2",
        "require": {
          "composer/installers": "^1.6.0"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    }
  ],
  "require": {
    "briancherne/jquery.hoverintent": "^1",
    "cdnjs/json2": "^1",
    "drupal/bootstrap": "^3",
    "drupal/config_update": "^1",
    "drupal/core": "^9",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/paragraphs": "^1",
    "drupal/redis": "^1",
    "drupal/simple_sitemap": "4.1.6",
    "drupal/views_slideshow": "^4",
    "malsup/jquery.cycle": "^1",
    "unb-libraries/drupal-builder": "^9"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
