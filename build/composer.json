{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of womenactivists.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "unb-libraries/womenactivists.lib.unb.ca-drupal8",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        },
        {
            "type": "git",
            "url": "https://github.com/components/jquery.git"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://raw.githubusercontent.com/malsup/cycle/master/jquery.cycle.all.js"
                },
                "name": "malsup/cycle",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "3.0.3"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://raw.githubusercontent.com/douglascrockford/JSON-js/master/json2.js"
                },
                "name": "douglascrockford/json2",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://raw.githubusercontent.com/tobia/Pause/master/jquery.pause.js"
                },
                "name": "tobia/Pause",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "0.1"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://raw.githubusercontent.com/briancherne/jquery-hoverIntent/master/jquery.hoverIntent.js"
                },
                "name": "briancherne/jquery-hoverIntent",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.9.0"
            },
            "type": "package"
        }
    ],
    "require": {
        "briancherne/jquery-hoverIntent": "1.9.0",
        "components/jquery": "3.3.1",
        "composer/installers": "^1.6.0",
        "cweagans/composer-patches": "1.6.5",
        "douglascrockford/json2": "1.0",
        "drupal-composer/drupal-scaffold": "2.5.4",
        "drupal/chosen": "2.1",
        "drupal/config_update": "1.5",
        "drupal/console": "~1.8.0",
        "drupal/core": "8.6.3",
        "drupal/features": "3.8",
        "drupal/inline_entity_form": "1.0-rc1",
        "drupal/paragraphs": "1.5",
        "drupal/views_slideshow": "4.6",
        "drush/drush": "^9.5.2",
        "malsup/cycle": "3.0.3",
        "tobia/Pause": "0.1",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
