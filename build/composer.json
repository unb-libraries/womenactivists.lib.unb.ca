{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of womenactivists.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/womenactivists.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://malsup.github.io/jquery.cycle.all.js"
                },
                "name": "malsup/jquery.cycle",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://briancherne.github.io/jquery-hoverIntent/jquery.hoverIntent.js"
                },
                "name": "briancherne/jquery.hoverIntent",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://cdnjs.cloudflare.com/ajax/libs/json2/20160511/json2.js"
                },
                "name": "cdnjs/json2",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.0"
            },
            "type": "package"
        }
    ],
    "require": {
        "briancherne/jquery.hoverIntent": "1.0",
        "cdnjs/json2": "1.0",
        "drupal/bootstrap": "3.19",
        "drupal/chosen": "2.1",
        "drupal/config_update": "1.5",
        "drupal/core": "8.7.1",
        "drupal/features": "3.8",
        "drupal/google_analytics": "2.4",
        "drupal/inline_entity_form": "1.0-rc1",
        "drupal/paragraphs": "1.8",
        "drupal/views_slideshow": "4.6",
        "malsup/jquery.cycle": "1.0",
        "unb-libraries/drupal-builder": "^8.0.0"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
