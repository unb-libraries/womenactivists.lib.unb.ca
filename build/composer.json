{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of womenactivists.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/womenactivists.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://malsup.github.io/jquery.cycle.all.js"
        },
        "name": "malsup/jquery.cycle",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/briancherne/jquery-hoverIntent/archive/v1.10.2.zip"
        },
        "extra": {
          "installer-name": "jquery.hover-intent"
        },
        "name": "jquery/hoverintent",
        "type": "drupal-library",
        "version": "1.10.2"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "file",
          "url": "https://cdnjs.cloudflare.com/ajax/libs/json2/20160511/json2.js"
        },
        "name": "cdnjs/json2",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "1.0"
      },
      "type": "package"
    }
  ],
  "require": {
    "cdnjs/json2": "^1",
    "drupal/bootstrap": "^3",
    "drupal/bootstrap5": "^3",
    "drupal/ckeditor": "^1",
    "drupal/color": "^1",
    "drupal/config_update": "^2",
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/paragraphs": "^1",
    "drupal/quickedit": "^1",
    "drupal/redis": "^1",
    "drupal/rdf": "^2",
    "drupal/simple_sitemap": "^4",
    "drupal/views_slideshow": "^5",
    "malsup/jquery.cycle": "^1",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}